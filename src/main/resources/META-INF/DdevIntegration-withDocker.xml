<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <applicationService
                serviceImplementation="de.php_perfect.intellij.ddev.docker_compose.DockerComposeCredentialProviderImpl"
                serviceInterface="de.php_perfect.intellij.ddev.docker_compose.DockerComposeCredentialProvider"/>

        <projectService serviceImplementation="de.php_perfect.intellij.ddev.docker_compose.DdevComposeFileLoaderImpl"
                        serviceInterface="de.php_perfect.intellij.ddev.docker_compose.DdevComposeFileLoader"/>
    </extensions>
</idea-plugin>
